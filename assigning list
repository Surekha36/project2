from itertools import compress 
 
test_list = [1, 4, 5, 6, 7, 3] 
  
print ("The original list is : " + str(test_list)) 
  
var1, var2, var3 = compress(test_list, (0, 1, 0, 1, 0, 1, 0)) 
  
print ("The variables are : " +  str(var1) + 
                           " " + str(var2) +
                            " " + str(var3)) 
